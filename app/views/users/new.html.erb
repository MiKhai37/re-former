<h1>New User: </h1>

<%= form_with model: @user do |form| %>
  <%= form.label(:username, "Username:") %>
  <%= form.text_field :username %> <br>
  <%= form.label(:email, "Email:") %>
  <%= form.email_field :email %> <br>
  <%= form.label(:password, "Password:") %>
  <%= form.password_field :password %> <br>
  <%= form.submit %>
<% end %>
